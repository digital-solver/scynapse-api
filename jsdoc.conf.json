{
  "recurseDepth": 10,
  "source": {
    "include": [
      "index.js",
      "auth.js",
      "models.js",
      "passport.js"
    ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}
