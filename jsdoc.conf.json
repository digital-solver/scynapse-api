{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "index.js",
      "auth.js",
      "models.js",
      "passport.js"
    ],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}
